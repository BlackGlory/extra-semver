{
  "name": "extra-semver",
  "version": "0.2.3",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-semver.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --runInBand --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --runInBand --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "husky": "4",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "return-style": "^1.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@blackglory/errors": "^2.3.0",
    "justypes": "^4.4.1",
    "semver": "^7.3.8"
  }
}
